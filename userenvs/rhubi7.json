{
  "workshop": {
    "schema": {
      "version": "2020.03.02"
    }
  },
  "userenv": {
    "name": "rhubi7",
    "label": "Red Hat UBI 7",
    "origin": {
      "image": "registry.access.redhat.com/ubi7/ubi",
      "tag": "latest"
    },
    "properties": {
      "packages": {
        "type": "rpm",
        "manager": "yum"
      }
    }
  },
  "requirements": [
    {
      "name": "jq",
      "type": "distro",
      "distro_info": {
        "packages": [
          "https://download-ib01.fedoraproject.org/pub/epel/7/x86_64/Packages/o/oniguruma-6.8.2-1.el7.x86_64.rpm",
          "https://download-ib01.fedoraproject.org/pub/epel/7/x86_64/Packages/j/jq-1.6-2.el7.x86_64.rpm"
        ]
      }
    },
    {
      "name": "diffutils",
      "type": "distro",
      "distro_info": {
        "packages": [
          "diffutils"
        ]
      }
    },
    {
      "name": "libtool",
      "type": "distro",
      "distro_info": {
        "packages": [
          "libtool"
        ]
      }
    },
    {
      "name": "automake",
      "type": "distro",
      "distro_info": {
        "packages": [
          "automake"
        ]
      }
    },
    {
      "name": "autoconf",
      "type": "distro",
      "distro_info": {
        "packages": [
          "autoconf"
        ]
      }
    },
    {
      "name": "curl",
      "type": "distro",
      "distro_info": {
        "packages": [
          "curl"
        ]
      }
    },
    {
      "name": "xz",
      "type": "distro",
      "distro_info": {
        "packages": [
          "xz"
        ]
      }
    },
    {
      "name": "gzip",
      "type": "distro",
      "distro_info": {
        "packages": [
          "gzip"
        ]
      }
    },
    {
      "name": "tar",
      "type": "distro",
      "distro_info": {
        "packages": [
          "tar"
        ]
      }
    },
    {
      "name": "python3",
      "type": "distro",
      "distro_info": {
        "packages": [
          "rh-python36",
          "rh-python36-python-pip"
        ]
      }
    },
    {
      "name": "post-python",
      "type": "manual",
      "manual_info": {
        "commands": [
          "alternatives --install /usr/bin/python3 python3 /opt/rh/rh-python36/root/usr/bin/python3 1",
          "alternatives --install /usr/bin/pip3 pip3 /opt/rh/rh-python36/root/usr/bin/pip3 1"
        ]
      }
    },
    {
      "name": "gcc",
      "type": "distro",
      "distro_info": {
        "packages": [
          "gcc"
        ]
      }
    },
    {
      "name": "make",
      "type": "distro",
      "distro_info": {
        "packages": [
          "make"
        ]
      }
    },
    {
